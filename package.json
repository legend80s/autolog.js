{
  "name": "antd-message-mini",
  "version": "2.2.1",
  "description": "lightweight message toast, imitation of antd message",
  "main": "index.js",
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/legend80s/antd-message-mini.git"
  },
  "files": [
    "es",
    "antd-message-mini",
    "index.js",
    "index.d.ts",
    "LICENSE",
    "README.md"
  ],
  "homepage": "https://legend80s.github.io/legend80s/antd-message-mini/",
  "scripts": {
    "cleanCSS": "node ./src/minify.js",
    "clean": "shx rm -rf ./es",
    "lint": "eslint src/**/*.ts",
    "dev": "webpack serve -c ./build/webpack.dev.conf.js",
    "dev2": "npm run cleanCSS && webpack serve -c ./build/webpack.dev.conf.js",
    "build": "node ./src/minify.js && npm run lint && npm run clean && npm run build:es && npm run build:node && npm run build:web",
    "build:es": "tsc --project ./tsconfig.es.json && tsc-alias -p tsconfig.es.alias.json",
    "build:node": "tsc --project ./tsconfig.node.json && tsc-alias -p tsconfig.node.alias.json",
    "build:web": "webpack -c ./build/webpack.prod.conf.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "prompt",
    "toast",
    "antd",
    "popup",
    "message",
    "feedback",
    "notification",
    "antd-message",
    "antd-message-mini"
  ],
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.5",
    "@types/jest": "29.2.4",
    "@types/jquery": "^3.5.30",
    "@types/node": "^20.2.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-jest": "^29.4.3",
    "babel-loader": "^9.1.2",
    "clean-css": "^5.3.3",
    "eslint": "^8.0.0",
    "eslint-webpack-plugin": "^4.2.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.4.3",
    "jquery": "^3.7.1",
    "lint-staged": "^10",
    "raw-loader": "^4.0.2",
    "shx": "^0.3.4",
    "terser-webpack-plugin": "^5.3.7",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.2",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "^4.4.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.10.0"
  },
  "author": "legend80s",
  "license": "MIT"
}
